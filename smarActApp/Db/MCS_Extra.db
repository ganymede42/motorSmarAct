record(bo, "$(P)$(M)cmdCal") {
    field(DESC,"cmd calibration")
    field(DTYP,"asynInt32")
    field(OUT, "@asyn($(PORT),$(ADDR),$(TIMEOUT))CAL")
}

record(mbbo, "$(P)$(M)setPositionerType"){
  field(DESC, "set positioner type")
  field(DTYP, "asynInt32")
  field(SCAN, "Passive")
  field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))PTYP")
  field(NOBT, "11")
  field(ZRVL, "1")
  field(ONVL, "2")
  field(TWVL, "5")
  field(THVL, "6")
  field(FRVL, "8")
  field(FVVL, "9")
  field(SXVL, "21")
  field(SVVL, "23")
  field(EIVL, "32")
  field(NIVL, "18")
  field(TEVL, "1")
  field(ELVL, "38")
  field(TVVL, "41")
  field(ZRST, "S")
  field(ONST, "SR")
  field(TWST, "SP")
  field(THST, "SC")
  field(FRST, "SR20")
  field(FVST, "M")
  field(SXST, "SD")
  field(SVST, "SR2")
  field(EIST, "MD")
  field(NIST, "SC500")
  field(TEST, "None")
  field(ELST, "LC")
  field(TVST, "L")
  field(VAL,  "0")
}

